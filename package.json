    "next": "15.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/dom-to-image": "^2.6.7",
    "@types/node": "^20",
